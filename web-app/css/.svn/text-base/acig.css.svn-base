/* This is the main css for the web application.  It can be overriden dynamically to customize by affiliate */
/* spacing parameters like margin and padding are top right bottom left */
/* Colors
   YELLOW         #FFDB6D
   GREEN          #445E07
   BROWN          #5E4F3A
   RED            #70070C
   DARK BROWN     #372113
*/
body {
    font-family: Verdana, Arial, Helvetica, San-Serif;
    font-size: 12px;
    font-weight: normal;
}

#header {
    width: 950px;
    margin: 0 auto;
}

#top-bar {
    height: 100px;
    font-size: .75em;
    font-weight: bold;
    background-color: #5E4F3A;

}

#logo {
    height: 100px;
    width: 250px;
    float: left;
    background: url( ../images/acig_logo.jpg ) top left no-repeat;

}

div#logo {
    margin: 0px 0px 0px 1px;
}

#right-side {
    float: right;
    padding-top: 15px;
    margin-right: 10px;
}

#right-side img {
    vertical-align: middle;
    border: 1px solid white;
}

#right-side a {
    height: 10px;
    padding-left: 10px;
    color: white;
    border-left: 1px solid white;
}

#right-side a.first {
    border: none;
    padding: 0;
}

.welcome {
    font-weight: bold;
}

#menu-bar {
    height: 32px;
    background-color: #cb842e;
}

/* End of Header */

/* Begin Logon */

#login {
    margin: 15px 0px;
    padding: 0px;
    text-align: center;
}

#login .inner {
    width: 260px;
    margin: 0px auto;
    text-align: left;
    padding: 10px;
    background-color: white;
}

#login .inner .fheader {
    padding: 4px;
    margin: 3px 0px 3px 0;
    font-size: 14px;
    font-weight: bold;
}

#login .inner .cssform p {
    clear: left;
    margin: 0;
    padding: 5px 0 8px 0;
    padding-left: 105px;
    margin-bottom: 10px;
    height: 1%;
}

#login .inner .cssform input[type='text'] {
    width: 120px;
}

#login .inner .cssform label {
    font-weight: bold;
    float: left;
    margin-left: -105px;
    width: 100px;
}

#login .inner .login_message {
    color: red;
}

#login .inner .text_ {
    width: 120px;
}

#login .inner .chk {
    height: 12px;
}

/* End Login */
.spinner {
    padding: 0px 0px;
    margin: 0px 0px;
    position: absolute;
    left: 50%;
    top: 50%;
    width: 400;
    height: 400;
    overflow: auto;
    z-index: 2000;
}

/* PAGINATION */

.paginateButtons {
    background: #fff url( ../images/skin/shadow.jpg ) bottom repeat-x;
    border: 1px solid #ccc;
    border-top: 0;
    color: #666;
    font-size: 10px;
    overflow: hidden;
    padding: 10px 3px;
}

.paginateButtons a {
    background: #fff;
    border: 1px solid #ccc;
    border-color: #ccc #aaa #aaa #ccc;
    color: #666;
    margin: 0 3px;
    padding: 2px 6px;
}

.paginateButtons span {
    padding: 2px 3px;
}

/* End Paginate Buttons */

/* Begin messages and errors defintions */
.message {
    background: #f3f8fc url( ../images/skin/information.png ) 8px 50% no-repeat;
    width: 915px;
    margin: 0 auto;
    border: 1px solid #b2d1ff;
    color: #006dba;
    padding: 5px 5px 5px 30px
}

.error {
    background: #fff3f3 url( ../images/skin/exclamation.png ) 8px 50% no-repeat;
    width: 915px;
    margin: 0 auto;
    border: 1px solid red;
    color: #cc0000;
    padding: 5px 5px 5px 30px
}

.errors {
    border: 1px solid red;
}

div.errors {
    background: #fff3f3;
    border: 1px solid red;
    color: #cc0000;
    margin: 10px 0 5px 0;
    padding: 5px 0 5px 0;
}

div.errors ul {
    list-style: none;
    padding: 0;
}

div.errors li {
    background: url( ../images/skin/exclamation.png ) 8px 0% no-repeat;
    line-height: 16px;
    padding-left: 30px;
}

td.errors select {
    border: 1px solid red;
}

td.errors input {
    border: 1px solid red;
}

/* End messages and errors definitions */

/* begin ticker declarations */

.tickercontainer {
/* the outer div with the black border */
    border: 1px solid #000;
    background: #fee4bd;
    width: 948px;
    height: 27px;
    margin: 0;
    padding: 0;
    overflow: hidden;
}

.tickercontainer .mask {
/* that serves as a mask. so you get a sort of padding both left and right */
    position: relative;
    left: 4px;
    top: 5px;
    width: 940px;
    height: 17px;
    overflow: hidden;
}

ul.newsticker {
/* that's your list */
    position: relative;
    left: 750px;
    font: bold 10px Verdana;
    list-style-type: none;
    margin: 0;
    padding: 0;
}

ul.newsticker li {
    float: left; /* important: display inline gives incorrect results when you check for elem's width */
    margin: 0;
    padding: 0;
    background: #fee4bd; /* This fixes a wrapping issue on IE */
    border: 1px solid #fee4bd;

}

ul.newsticker a {
    white-space: nowrap;
    padding: 0;
    color: blue;
    font: bold 10px Verdana;
    margin: 0 50px 0 0;
}

ul.newsticker span {
    margin: 0 10px 0 10px;
}

/* end ticker declarations */

.required {
    color: orange;
}

/* Begin Form Element Definitions */

.form-element {
    padding: 2px 0px 0px 0px;
}

.output-label-element {
    font-weight: bold;
    width: 50%;
    float: left;
}

.output-value-element {
    font-weight: normal;
    width: 50%;
}

.output-wrapped-label-element {
    font-weight: bold;
}

.output-wrapped-value-element {
    font-weight: normal;
}

.input-label-element {
    font-weight: bold;
}

.input-value-element {
    font-weight: normal;
}

/* Begin General Definitions */

.textarea {
    width: 940px;
    height: 80px;
}

thead th {
    background-color: #cb842e
}

tr.odd td {
    background-color: #fee4bd;
}

tr.even td {
